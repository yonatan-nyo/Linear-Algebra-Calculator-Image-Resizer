<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chisli.RegresiBergandaController">
    <children>
        <Label text="Regresi Berganda" />
        <Button fx:id="primaryButton" text="Switch to Sistem Persamaan Linear" onAction="#switchToSistemPersamaanLinear"/>
        <Button fx:id="primaryButton" text="Switch to Interpolasi Polinomial" onAction="#switchToInterpolasiPolinomial"/>
        <Button fx:id="primaryButton" text="Switch to Regresi Berganda " onAction="#switchToRegresiBerganda"/>
        <Button fx:id="primaryButton" text="Switch to Bicubic Spline Interpolation" onAction="#switchToBicubicSplineInterpolation"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
